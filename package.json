{
  "name": "compat-db",
  "version": "0.0.1-0",
  "description": "A central and scalable browser api compatibility database",
  "main": "lib/src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amilajack/compat-db.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rm -rf lib && mkdir lib && babel src/index.js --out-file lib/index.js",
    "build-compat-db-webdriverio": "node -r babel-register compat-tests-webdriver/setup.js && wdio wdio.conf.js",
    "build-compat-db": "node -r babel-register compat-tests/setup.js && node -r babel-register compat-tests/Compat.js",
    "dir-remove": "rm -rf tmp-db-records && mkdir tmp-db-records && touch tmp-db-records/database.sqlite",
    "flow": "flow check",
    "flow-typed": "flow-typed install",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "migrate": "npm run dir-remove && babel-node ./src/database/Migrate",
    "spec": "cross-env NODE_ENV=test npm run dir-remove && npm run migrate && jest test/*.js",
    "test": "cross-env NODE_ENV=development npm run lint && npm run spec",
    "update-catalog-api-db": "download https://raw.githubusercontent.com/MicrosoftEdge/APICatalogData/master/apicatalogdata.json && mkdir -p src/providers/api-catalog && mv apicatalogdata.json src/providers/api-catalog/apicatalogdata.json",
    "version": "npm run build-compat-db && npm run build"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "api",
    "compat",
    "caniuse",
    "support",
    "database",
    "db"
  ],
  "author": "Amila Welihinda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amilajack/compat-db/issues"
  },
  "files": [
    "databases"
  ],
  "homepage": "https://github.com/amilajack/compat-db#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-flow-runtime": "^0.5.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "bookshelf": "^0.10.3",
    "caniuse-db": "1.0.30000623",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "dotenv": "^4.0.0",
    "download-cli": "^1.0.1",
    "eslint": "^3.15.0",
    "eslint-config-bliss": "^0.0.7",
    "eslint-formatter-pretty": "^1.1.0",
    "flow-bin": "^0.39.0",
    "flow-runtime": "^0.5.0",
    "flow-typed": "^2.0.0",
    "jest-cli": "^18.1.0",
    "knex": "^0.12.7",
    "mysql": "^2.13.0",
    "nightmare": "^2.9.1",
    "selenium-webdriver": "^3.1.0",
    "sequelize": "^3.30.2",
    "sqlite3": "^3.1.8",
    "tcomb": "^3.2.16"
  },
  "engines": {
    "node": ">=4.x",
    "npm": ">=3.x"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=4.x"
  }
}
